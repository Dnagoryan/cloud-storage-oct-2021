<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="647.0" prefWidth="745.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.geekbrains.network.controllers.ChatController">
   <children>

      <ListView fx:id="listViewClient" layoutX="14.0" layoutY="121.0" prefHeight="475.0" prefWidth="312.0" />
      <TextArea fx:id="absolutPathClient" layoutX="14.0" layoutY="86.0" prefHeight="36.0" prefWidth="312.0" />
      <TextArea fx:id="fileNameClient" layoutX="14.0" layoutY="596.0" prefHeight="36.0" prefWidth="312.0" />


      <Button fx:id="upload" layoutX="336.0" layoutY="124.0" mnemonicParsing="false" onAction="#uploadFile" prefHeight="36.0" prefWidth="66.0" text="Upload" />
      <Button fx:id="download" layoutX="333.0" layoutY="560.0" mnemonicParsing="false" onAction="#download" prefHeight="36.0" prefWidth="71.0" text="Download" />

      <ListView fx:id="listViewServer" layoutX="416.0" layoutY="124.0" prefHeight="475.0" prefWidth="312.0" />
      <TextArea fx:id="fileNameServer" layoutX="416.0" layoutY="597.0" prefHeight="36.0" prefWidth="312.0" />
      <Button layoutX="124.0" layoutY="35.0" mnemonicParsing="false" onAction="#toDirectory" text="--&gt;" />
      <Button layoutX="50.0" layoutY="35.0" mnemonicParsing="false" onAction="#fromDirectory" text="&lt;--" />
      <Button layoutX="213.0" layoutY="35.0" mnemonicParsing="false" onAction="#deleteFileFromClient" text="Delete from Client" />
      <Button onAction="#deleteFileFromServer" layoutX="495.0" layoutY="86.0" mnemonicParsing="false" text="Delete from Server" />


   </children>
</AnchorPane>
